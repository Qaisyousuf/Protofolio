                        <div class="font-weight-bold display-5">@item.MainTitle</div>
                    }


                    <!-- Section description -->
                    <div class="container">
                        <div class="row ">

                            <div class="col-md-5 col-sm-6 offset-1 mt-5">
                                <div class="row text-center d-flex justify-content-center">
                                    @foreach (var SubItem in Model.ListInterestViewModel)
                                    {
                                        <div class="col-sm-8 col-md-6 col-lg-4 mb-4">
                                            <div class=" mx-auto mb-2">

                                                <div class="interestSVG">
                                                    <div class="MainSVGBack">
                                                        <lottie-player src=https://assets9.lottiefiles.com/packages/lf20_xZ8sgo.json background="transparent" speed="1" style="width: 100px; height: 100px;" loop autoplay></lottie-player>

                                                    </div>
                                                    <div class="MainIcon">
                                                        <img src="@SubItem.UrlIcon" class="img-fluid" alt="smaple image">
                                                    </div>
                                                </div>


                                            </div>
                                            <h5 class="font-weight-bold text-white mb-4">@SubItem.Subtitle</h5>

                                        </div>
                                    }


                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="row text-center d-flex justify-content-center">
                                    <div class="col-md-12 mb-4">
                                        <div class="img-fluid py-3">
                                            <lottie-player src=https://assets4.lottiefiles.com/datafiles/MUp3wlMDGtoK5FK/data.json background="transparent" speed="3" style="width: auto; height: auto;" loop autoplay></lottie-player>

                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>



                </section>
                <!--Section: Content-->


            </div>
        </div>
    }

