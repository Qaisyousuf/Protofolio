                                    </div>
                                    <!--Grid column-->
                                    <!--Grid column-->
                                    <div class="col-lg-5">
                                        <div class="img-fluid pt-5">
                                            <lottie-player src=@item.BackgroundImage background="transparent" speed="1" style="width: auto; height: auto;" loop autoplay></lottie-player>

                                        </div>
                                    </div>
                                    <!--Grid column-->
                                }

                            </div>

                        <!--Grid row-->

                        <!--Grid row-->
                    </div>
                    <!-- Content -->
                </div>
                <!-- Mask & flexbox options-->
            </div>
            <!-- Full Page Intro -->

        </div>
        <!-- Main navigation -->

    </div>
        <!-- Main navigation -->

