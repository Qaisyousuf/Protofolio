        @if (Model?.ListPortfolioProject != null && Model?.ListProjectStatuesViewModel != null)
        {

            <div class="PortfolioProjectMainBack">
                <div class="container my-5 mx-md-5 mx-sm-0 mx-lg-5">


                    <!--Section: Content-->
                    <section class="dark-grey-text offset-lg-2">


                        <!-- Section heading -->
                        @foreach (var item in Model.ListPortfolioProject.Take(1))
                        {
                            <div class="text-center display-4 font-weight-bold mb-4 pb-2">@item.MainTitle</div>
                        }

                        <!-- Section description -->
                        <!-- Grid row -->
                        <div class="row align-items-center">

                            <!-- Grid column -->
                            @foreach (var SubItem in Model.ListPortfolioProject)
                            {
                                <div class="col-md-4 col-xl-4 py-5">

                                    <!-- Featured image -->
                                    <div class=" overlay rounded z-depth-0 mb-lg-0 mb-4">
                                        <div class="img-fluid py-3">
                                            <lottie-player src=@SubItem.ProjectImageUrl background="transparent" speed="1" style="width: auto; height: auto;" loop autoplay></lottie-player>

                                        </div>
                                        <a>
                                            <div class="mask rgba-white-slight"></div>
                                        </a>
                                    </div>

                                </div>
                                <!-- Grid column -->
                                <!-- Grid column -->
                                <div class="col-md-7 col-xl-8">

                                    <!-- Post title -->
                                    <h3 class="font-weight-bold mb-3 light-blue-text">@SubItem.ProjectName</h3>
                                    <!-- Excerpt -->
                                    <p class="dark-grey-text">
                                        @SubItem.Content
                                    </p>
                                    <!-- Post data -->

                                    <div class="chip btn-primary">
                                        Date:  @SubItem.ProjectPublishDate.ToShortDateString().ToString()
                                        <i class="close fas fa-times"></i>
                                    </div>
                                    <!-- Read more button -->

                                    <div class="chip btn-primary">
                                        Type: @SubItem.ProjectType
                                        <i class="close fas fa-times"></i>
                                    </div>
                                    <div class="chip btn-primary">
                                        Created by: @SubItem.ProjectDetails
                                        <i class="close fas fa-times"></i>
                                    </div>
                                    @foreach (var ProjectStatusItem in Model.ListProjectStatuesViewModel.Take(1))
                                    {
                                        <div class="chip btn-primary">
                                            Status: @ProjectStatusItem.ProjectStatusProcess
                                            <i class="close fas fa-times"></i>
                                        </div>
                                    }

                                    <br />
                                    <a href="@SubItem.ProjectWebSiteUrl" class="btn btn-primary btn-md mx-0 btn-rounded" target="_blank">@SubItem.ButtonText</a>
                                </div>
                            }

                            <!-- Grid column -->

                        </div>
                        <!-- Grid row -->



                    </section>
                    <!--Section: Content-->


                </div>
            </div>

        }

    @if (Model?.ListInterestViewModel != null)
    {
        <div class="interestMainBackground">
            <div class="container  px-5 pb-3 z-depth-0">


                <!--Section: Content-->
                <section class="text-center dark-grey-text offset-lg-2">


