@model QaisYousuf.ViewModels.BaseViewModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - @Model.BaseTitle</title>
    <link href="https://fonts.googleapis.com/css?family=Ubuntu:400,500&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css">
  
    <link href="~/Content/slick.css" rel="stylesheet" />
    <link href="~/Content/slick-theme.css" rel="stylesheet" />
   

    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/AboutCustomCSS.css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")

    <script src="~/Scripts/node_modules/lottie-player/dist/lottie-player.js"></script>







    <script src="~/Scripts/jquery-ui-1.12.1.min.js"></script>


    <script type="text/javascript" language="javascript" src="https://nightly.datatables.net/responsive/js/dataTables.responsive.min.js">
    </script>
    <script type="text/javascript" language="javascript" src="https://nightly.datatables.net/js/jquery.dataTables.min.js">
    </script>

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.16/css/dataTables.bootstrap.min.css" />
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.16/js/dataTables.bootstrap.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.4.2/js/dataTables.buttons.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.4.2/js/buttons.print.js"></script>

    <script type="text/javascript" src="https://cdn.datatables.net/responsive/2.2.3/js/dataTables.responsive.min.js"></script>
    <script type="text/javascript" src=" https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.1/js/dataTables.buttons.min.js"></script>

    <script type="text/javascript" src="  https://cdn.datatables.net/buttons/1.6.1/js/buttons.flash.min.js"></script>
    <script type="text/javascript" src="  https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>

    <script type="text/javascript" src=" https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src=" https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src=" https://cdn.datatables.net/buttons/1.6.1/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="  https://cdn.datatables.net/buttons/1.6.1/js/buttons.print.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.1/js/buttons.colVis.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/1.6.1/js/dataTables.buttons.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.16/css/jquery.dataTables.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/1.4.2/css/buttons.dataTables.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment-with-locales.min.js"></script>
    @*<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.18.1/moment.min.js"></script>*@
    <script src="https://cdn.datatables.net/select/1.3.1/js/dataTables.select.min.js"></script>
    <script src="https://cdn.datatables.net/plug-ins/1.10.19/dataRender/datetime.js"></script>



</head>

<body>
    <!--Main Navigation-->
    <!--Main Navigation-->







    <header>

        <nav class="navbar fixed-top navbar-expand-lg navbar-dark scrolling-navbar ">

            <a class="navbar-brand" href="#"><strong>UI UX To Code</strong></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon btn btn-primary btn-rounded btn-indigo"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                @Html.Action("PartialMenu", "Home")
                @Html.Partial("_LoginPartial")
            </div>
        </nav>
    </header>


    <div class="body-content">
        @RenderSection("CodeCustomCSS", required: false)
       
        @RenderBody()
        

    </div>
    <footer>

        <div class="mt-0">

            <div class="footerwaves mt-5">

                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1440 320"><path fill="#169cba" fill-opacity="1" d="M0,128L48,138.7C96,149,192,171,288,165.3C384,160,480,128,576,138.7C672,149,768,203,864,186.7C960,171,1056,85,1152,58.7C1248,32,1344,64,1392,80L1440,96L1440,320L1392,320C1344,320,1248,320,1152,320C1056,320,960,320,864,320C768,320,672,320,576,320C480,320,384,320,288,320C192,320,96,320,48,320L0,320Z"></path></svg>
            </div>
            <!--Section: Content-->

            <div class="px-md-0 mx-md-0 pb-2 text-center text-white" style="background-color:#169cba;">
                <div class="col-lg-7 col-md-6 col-sm-12 mx-0 font-weight-bold pb-2 text-lg-left text-md-center text-sm-center">
                    <div class="display-3 text-uppercase font-weight-bold">ui to code</div>
                    <h3>@Model.BaseContent</h3>
                </div>


                <div class="container my-0 py-0 z-depth-0 text-white">


                    <!--Section: Content-->
                    <section class="dark-grey-text text-center">



                        <div class="row mx-3">
                            <div class="col-md-5 px-4 mb-4">

                                <div class="">
                                    <div class="img-fluid">
                                        <lottie-player src="https://assets6.lottiefiles.com/packages/lf20_KxADqK.json" background="transparent" speed="1" style="width: auto; height: auto;" loop autoplay></lottie-player>
                                    </div>
                                </div>

                            </div>
                            <div class="col-md-2 px-4 mb-0 mt-lg-5  mt-md-5 mt-sm-0 pt-lg-5 pt-md-5 pt-sm-0 text-white">

                                <ul class="list-unstyled font-weight-bold mt-lg-5 mt-md-5 mt-sm-0">
                                    <li>
                                        <a class="text-white-50" href=@Model.HomeUrl>@Model.Home</a>
                                    </li>
                                    <li>
                                        <a class="text-white-50" href=@Model.AboutUrl>@Model.About</a>
                                    </li>
                                    <li>
                                        <a class="text-white-50" href=@Model.ContactUrl>@Model.Contact</a>
                                    </li>
                                    <li>
                                        <a class="text-white-50" href=@Model.UIUX>@Model.UIUX</a>
                                    </li>
                                    <li>
                                        <a class="text-white-50" href=@Model.CodeUrl>@Model.Code</a>
                                    </li>
                                    <li>
                                        <a class="text-white-50" href=@Model.Support>@Model.Support</a>
                                    </li>
                                    <li>
                                        <a class="text-white-50" href=@Model.ProfileUrl>@Model.Profile</a>
                                    </li>
                                </ul>


                            </div>
                            <div class="col-md-5 px-4 mb-4">

                                <div class="view">
                                    <img src="~/Content/Images/girl-sitiing-on-table-conducting-an-interview-by-oblik-studio.svg" class="img-fluid" alt="smaple image">
                                </div>

                            </div>
                        </div>

                    </section>
                    <!--Section: Content-->


                </div>

                <section>
                    <div class="footer-copyright text-center mt-0 mb-0">
                        <p class="text-white-50 mb-1">© 2018 Copyright:@Model.SiteName</p>
                        <p class="text-white-50 mb-1">Site Last Updated: @Model.SiteLastUpdate</p>
                        <p class="text-white-50 mb-0">@Model.CopyrightFooter</p>

                    </div>
                </section>

            </div>
            <!--Section: Content-->


        </div>


    </footer>

    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/popper")

    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/slick.min.js"></script>


</body>
</html>