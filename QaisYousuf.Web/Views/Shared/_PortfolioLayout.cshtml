                                <div class="col-md-6">
                                    <div class="display-1 text-uppercase font-weight-bold">ui to code</div>
                                    <h3 class="text-white">@Model.BaseContent</h3>
                                    <a href="@Model.ContactUrl" class="btn btn-primary btn-rounded">@Model.Contact</a>
                                </div>
                                <!--Grid column-->
                                <!--Grid column-->
                                <div class="col-md-6">
