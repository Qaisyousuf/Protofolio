                                <!-- Content -->
                                <div class="container-fluid">
                                    <!--Grid row-->
                                    <div class="row align-items-center mx-md-3 mx-sm-0 mx-lg-2">
                                        <!--Grid column-->
                                        <div class="col-md-6">
                                            <div class="display-5 text-uppercase font-weight-bold">ui to code</div>
                                            <h3 class="text-white">@Model.BaseBaseContent</h3>
                                            <a href="@Model.BaseBaseContactUrl" class="btn btn-primary btn-rounded">@Model.BaseContact</a>
                                        </div>
                                        <!--Grid column-->
                                        <!--Grid column-->
                                        <div class="col-md-6">
                                            <div class="view">
                                                <lottie-player src=https://assets1.lottiefiles.com/packages/lf20_gzl797gs.json background="transparent" speed="0.5" style="width: auto; height: auto;" loop autoplay></lottie-player>
                                            </div>
                                        </div>
                                        <!--Grid column-->
                                    </div>
                                    <!--Grid row-->
                                </div>
                                <!-- Content -->
                            </div>
                            <!-- Mask & flexbox options-->
                        </div>
                        <!-- Full Page Intro -->

                    </div>
                    <!-- Main navigation -->

                    <div class="px-md-0 mx-md-0  mb-lg-2 mb-sm-5 text-white">


                        <section>
