        <!--Double navigation-->
        <header>
            <!-- Sidebar navigation -->
            <div id="slide-out" class="side-nav fixed">
                <ul class="custom-scrollbar">
                    <!-- Logo -->
                    <li>
                        <div class="logo-wrapper waves-light">

                            <lottie-player src=https://assets3.lottiefiles.com/packages/lf20_uxphx9ac.json background="transparent" speed="1" style="width: 200px; height: 200px;" loop autoplay></lottie-player>

