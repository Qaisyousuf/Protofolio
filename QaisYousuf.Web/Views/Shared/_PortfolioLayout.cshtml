                <section>
                    <div class="footer-copyright text-center mt-0 mb-0">
                        <p class="text-white-50 mb-1">© 2018 Copyright:@Model.SiteName</p>
                        <p class="text-white-50 mb-1">Site Last Updated: @Model.SiteLastUpdate</p>
                        <p class="text-white-50 mb-0">@Model.CopyrightFooter</p>

                    </div>
                </section>

            </div>
            <!--Section: Content-->


        </div>


